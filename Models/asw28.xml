    <!--PushToTalk Front--> 
    <animation>
        <type>pick</type>
        <object-name>ptt-front</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/instrumentation/comm[0]/ptt</property>
            </binding>
            <mod-up>
                <binding>
                    <command>property-toggle</command>
                    <property>/instrumentation/comm[0]/ptt</property>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>ptt-front</tooltip-id>
                <label>Push to talk: %s</label>
                <mapping>on-off</mapping>
                <property>/instrumentation/comm[0]/ptt</property>
            </binding>
        </hovered>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>ptt-front</object-name>
        <property>/instrumentation/comm[0]/ptt</property>
        <factor>-0.002</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
        <!--Spoiler Handle-->
        <animation>
            <type>slider</type>
            <object-name>airbrake-lever</object-name>
            <visible>true</visible>
            <drag-direction>horizontal</drag-direction>
            <action>
                <button>0</button>
                <repeatable>false</repeatable>
                <binding>
                    <command>property-adjust</command>
                    <property>/controls/engines/engine/throttle</property>
                    <factor>0.025</factor>
                    <min>0</min>
                    <max>1</max>
                </binding>
            </action>
            <hovered>
                <binding>
                    <command>set-tooltip</command>
                    <tooltip-id>airbrake</tooltip-id>
                    <label>%s</label>
                    <mapping>nasal</mapping>
                    <property>/controls/engines/engine/throttle</property>
                    <script>
                        return sprintf("Speedbrake %d%% out", 100-arg[0]*100);
                    </script>
                </binding>
            </hovered>
        </animation>
 	 <animation>
            <type>translate</type>
            <object-name>airbrake-lever</object-name>
            <property>controls/engines/engine/throttle</property>
            <axis>
                    <x>1</x>
                    <y>0.0</y>
                    <z>0.2</z>
            </axis>
            <interpolation>
                <entry>
                    <ind>1</ind>
                    <dep>0</dep>
                </entry>
                <entry>
                    <ind>0.0</ind>
                    <dep>0.3</dep>
                </entry>
            </interpolation>
	</animation>
